{% extends 'base.html.twig' %}

{% block title %}View{% endblock %}

{% block body %}

<div class="container">

    <div class="my-4">
        <div class="row">
            <div class="col-lg-12">
                <div class="page-header">
                    <h1 id="tables">View</h1>
                </div>

                <div class="bs-component">
                    <ul>
                        {% for key, value in params %}
                            <li>{{ key }} : <b>{{ value }}</b></li>
                        {% endfor %}
                    </ul>

                </div>
            </div>
        </div>
    </div>
</div>

{% endblock %}
